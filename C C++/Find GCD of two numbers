
#include <stdio.h>

int main() {
    
    printf("Hello, World!\n");

    int n1,n2,i,gcd;
    printf("Enter two integers: ");
    scanf("%d %d",&n1,&n2);
    for(i=1; i<=n1 && i<=2;++i)
    {
        // checks if i is factor of n1 and n2
        if(n1 % i ==0 && n2%i==0)
            gcd =i;
    }
    printf("G.C.D of %d and %d is %d",n1,n2,gcd);
    return 0;
}



